{
  "display_information": {
    "name": "RankedChoice",
    "description": "A Slack app for conducting ranked choice voting in your workspace",
    "background_color": "#2c2d30"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": true
    },
    "bot_user": {
      "display_name": "RankedChoice",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "chat:write",
        "app_mentions:read",
        "channels:history",
        "groups:history",
        "im:history",
        "mpim:history",
        "users:read",
        "channels:read",
        "channels:join"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "https://d788-75-67-241-13.ngrok-free.app/slack/events",
      "bot_events": [
        "app_mention",
        "app_home_opened",
        "message.channels",
        "message.groups",
        "message.im",
        "message.mpim"
      ]
    },
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://d788-75-67-241-13.ngrok-free.app/slack/events"
    }
  }
}
